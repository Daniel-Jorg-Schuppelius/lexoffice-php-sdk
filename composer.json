{
    "name": "daniel-jorg-schuppelius/lexoffice-php-sdk",
    "description": "Lexoffice SDK for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Joerg Schuppelius",
            "email": "info@schuppelius.org"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1 || ^8.3",
        "guzzlehttp/guzzle": "^7.9",
        "psr/log": "^3.0@dev",
        "daniel-jorg-schuppelius/php-api-toolkit": "^0.3.6"
    },
    "autoload": {
        "psr-4": {
            "Lexoffice\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3@dev"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
